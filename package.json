{
  "name": "traders-vue",
  "private": true,
  "version": "13.02.2026",
  "scripts": {
    "serve": "npm run dev --debug",
    "dev": "vite --mode dev",
    "serve:dev": "vite --mode dev",
    "serve:stage": "vite --mode stage",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore --fix src",
    "format": "prettier .  --write",
    "build:dev": "vue-tsc --noEmit && vite build --mode dev",
    "build:stage": "vue-tsc --noEmit && vite build --mode stage",
    "build:paris-stage": "vue-tsc --noEmit && vite build --mode paris-stage",
    "build:prod": "vue-tsc --noEmit && vite build --mode prod",
    "build:thai-prod": "vue-tsc --noEmit && vite build --mode thai-prod",
    "build:thai-stage": "vue-tsc --noEmit && vite build --mode thai-stage",
    "build:archive:stage": "rm -rf build.tar.gz && npm run build:stage && tar -cvzf build.tar.gz dist",
    "build:archive:prod": "rm -rf build.tar.gz && npm run build:prod && tar -cvzf build.tar.gz dist",
    "build:archive:thai-prod": "rm -rf build.tar.gz && npm run build:thai-prod && tar -cvzf build.tar.gz dist",
    "build:archive:thai-stage": "rm -rf build.tar.gz && npm run build:thai-stage && tar -cvzf build.tar.gz dist"
  },
  "dependencies": {
    "@chenfengyuan/vue-countdown": "^2.1.2",
    "@fingerprintjs/fingerprintjs-pro-vue-v3": "^1.1.2",
    "@heroicons/vue": "^1.0.5",
    "@kyvg/vue3-notification": "^2.7.0",
    "@tailwindcss/forms": "^0.4.0",
    "@vuepic/vue-datepicker": "^4.0.1",
    "@vueup/vue-quill": "^1.1.0",
    "apexcharts": "^4.0.0",
    "axios": "^0.26.0",
    "big.js": "^6.2.2",
    "chart.js": "^3.7.1",
    "core-js": "^3.8.3",
    "dayjs": "^1.11.7",
    "flowbite": "^1.4.1",
    "lodash": "^4.17.21",
    "maska": "^1.5.0",
    "moment": "2.24.0",
    "moment-timezone": "^0.5.41",
    "perfect-scrollbar": "^1.5.5",
    "pinia": "^2.0.14",
    "primevue": "^3.49.1",
    "pusher-js": "^7.4.0",
    "qs": "^6.9.4",
    "smooth-scrollbar": "^8.7.4",
    "tw-elements": "^1.0.0-alpha12",
    "v-tables-3": "^0.4.7",
    "v3-infinite-loading": "^1.3.1",
    "vue": "^3.2.36",
    "vue-multiselect": "^3.0.0-alpha.2",
    "vue-router": "^4.0.15",
    "vue-universal-modal": "^1.1.4",
    "vue3-apexcharts": "^1.4.1",
    "vue3-clipboard": "^1.0.0",
    "vue3-perfect-scrollbar": "^1.6.1"
  },
  "devDependencies": {
    "@iconify/vue": "^3.2.1",
    "@types/node": "^17.0.25",
    "@typescript-eslint/eslint-plugin": "^5.3.0",
    "@typescript-eslint/parser": "^5.3.0",
    "@vitejs/plugin-vue": "^2.3.3",
    "autoprefixer": "10.4.5",
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^9.0.1",
    "postcss": "^8.4.14",
    "prettier": "2.6.2",
    "sass": "^1.71.1",
    "tailwindcss": "^3.0.24",
    "typescript": "^4.8.4",
    "vite": "^2.9.9",
    "vite-plugin-eslint": "^1.6.1",
    "vue-tsc": "^0.34.7"
  }
